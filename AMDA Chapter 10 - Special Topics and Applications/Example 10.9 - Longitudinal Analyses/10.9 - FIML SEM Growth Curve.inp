! EXAMPLE 10.9 - FIML SEM Growth Curve

DATA: 
file = drugtrial.dat;
VARIABLE:
names = id male drug severe0 severe1 severe3 severe6 
   dropgrp earlydrop latedrop dropout sdrop3 sdrop6;
usevariables = drug severe0 severe1 severe3 severe6;
missing = all(999);
ANALYSIS:
estimator = mlr;
MODEL:
icept slope | severe0@0 severe1@1 severe3@1.73 severe6@2.45;
icept slope on drug;
severe0-severe6 (res);
OUTPUT:
patterns sampstat stdyx;